{
  "version": 2,
  "name": "task-manager",
  "builds": [
    {
      "src": "dist/task-manager/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "API_URL": "https://your-backend-url.vercel.app"
  },
  "functions": {
    "src/backend/**/*.js": {
      "runtime": "nodejs18.x"
    }
  }
}
